{
  "name": "GosNotification",
  "version": "0.1.0",
  "description": "GosNotification Client Side of GosNotificationBundle",
  "main": "./src/gos-notification.js",
  "scripts": {
    "watchify": "watchify ./src/gos-notification.js -o ./dist/gos-notification-dist.js -v"
  },
  "author": "Johann Saunier <johann_27@hotmail.fr>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.3.15",
    "angular-animate": "^1.3.15",
    "angular-moment": "^0.10.0",
    "angular-toastr": "^1.2.1",
    "ng-audio": "0.0.2",
    "ng-scrollbar": "0.0.7"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "watchify": "^3.1.0"
  },
  "browser": {
    "angular-toastr": "./node_modules/angular-toastr/dist/angular-toastr.js",
    "angular-scrollbar": "./external/ng-scrollbar-fix.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular-toastr": { "exports": "angular.module('toastr').name" },
    "angular-scrollbar" : { "exports": "angular.module('ngScrollbar').name" }
  }
}
